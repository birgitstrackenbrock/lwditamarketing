      </xs:sequence>
      <xs:attributeGroup ref="benefit2section-attlist"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="benefit2section-attlist">
    <xs:attributeGroup ref="localization"/>
    <xs:attributeGroup ref="filters"/>
    <xs:attributeGroup ref="reuse"/>
    <xs:attribute name="class" default="- topic/section benefit2-d/benefit2section "/>
  </xs:attributeGroup>
  
  
